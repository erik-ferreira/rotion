{
  "extends": ["@rocketseat/eslint-config/react"],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "printWidth": 80,
        "tabWidth": 2,
        "singleQuote": false,
        "trailingComma": "es5",
        "arrowParens": "always",
        "semi": true,
        "endOfLine": "auto",
        "parser": "flow"
      }
    ]
  }
}
